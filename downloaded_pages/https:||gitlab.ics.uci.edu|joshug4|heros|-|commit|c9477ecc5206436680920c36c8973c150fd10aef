 switch(elementsRead) {










 case 0:










 elementsRead++;










 return first;










 case 1:










 elementsRead++;










 return second;










 default:










 throw new NoSuchElementException(); 










 }










 }





















 @Override










 public void remove() {










 throw new UnsupportedOperationException();










 }










 };










 }





















 @Override










 public int size() {










 return 2;










 } 










}












