        if V.length vv > 1 
            then Just (V.tail vv)
            else Nothing

(%) :: (Maybe Tabular) -> Int ->Maybe TabularRow
t % i = t >>= (%! i)


(%%!):: Tabular -> String -> Maybe TabularRow
t %%! s = let hd = header t in case hd of
         Nothing -> Nothing 
