-- File Accessor
fromFileWith' sep quote fp= do
    let myOption = CSVSettings{csvSep=sep, csvQuoteChar=quote}
    csvData <- readCSVFile myOption fp :: IO ( V.Vector([Text]) )
    return (Just ( V.map V.fromList csvData))

