 show NA = "NA"










 show Cybert {probe=p, genesym=sym, sample=ss, collection=col, mean=m,










 pval=pv, ratio=r, secondaryRefs=_, secondaryData=_,raw=_} = p++"\t"++(showJustS sym)++"\t"










