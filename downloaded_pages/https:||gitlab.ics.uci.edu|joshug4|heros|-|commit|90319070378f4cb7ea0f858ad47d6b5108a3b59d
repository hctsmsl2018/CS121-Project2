 nestedResolver.propagated = true;










 nestedResolvers.put(candidateAccPath, nestedResolver);





















 for(WrappedFact<Field, Fact, Stmt, Method> incFact: incomingFacts) {










 nestedResolver.addIncoming(incFact);










 } 










 }










