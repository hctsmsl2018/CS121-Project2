 <junitreport tofile="TESTS-TestSuites.xml" todir="reports">










 <fileset dir="reports">










 <include name="TEST-*.xml" />










 </fileset>










 <report format="frames" todir="reports" />










 </junitreport>










 </target>





















</project>
















