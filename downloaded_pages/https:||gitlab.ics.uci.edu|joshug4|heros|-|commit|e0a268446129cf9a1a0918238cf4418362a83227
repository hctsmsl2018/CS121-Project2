 private boolean isNullOrCallEdgeResolver(Resolver<Field, Fact, Stmt, Method> resolver) {










 if(resolver == null)










 return true;










 if(resolver instanceof CallEdgeResolver) {










 return !(resolver instanceof ZeroCallEdgeResolver);










 }










 return false;










 }










 










