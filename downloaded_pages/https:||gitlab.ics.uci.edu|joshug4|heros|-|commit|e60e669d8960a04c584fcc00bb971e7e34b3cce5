 if(otherIndex == otherTransition.transitionToIndex())










 continue;










 










 currIndex = transition.transitionToIndex();










 otherIndex = otherTransition.transitionToIndex();










 continue outer;










 }










 }










 }










 return false;










 }










 }










 










 private boolean hasAtLeastTheSameExclusionsAs(AccessPath<T> accPath) {










