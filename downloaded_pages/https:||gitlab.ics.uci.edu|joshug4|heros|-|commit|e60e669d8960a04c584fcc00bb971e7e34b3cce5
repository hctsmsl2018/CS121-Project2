 public static enum PrefixTestResult {










 GUARANTEED_PREFIX(3), POTENTIAL_PREFIX(2), NEEDS_RESOLVNG(1), NO_PREFIX(0);










 GUARANTEED_PREFIX(2), POTENTIAL_PREFIX(1), NO_PREFIX(0);










 










