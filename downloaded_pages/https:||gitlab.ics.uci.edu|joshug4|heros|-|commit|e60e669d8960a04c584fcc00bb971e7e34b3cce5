 if(accPath.accesses.length == 1) {










 for(SubAccessPath<T> sub : accesses) {










 if(sub.elements().equals(accPath.accesses[0].elements())) {










 for(T sub : accesses) {










 if(sub.equals(accPath.accesses[0])) {










 return true;










 }










 }



