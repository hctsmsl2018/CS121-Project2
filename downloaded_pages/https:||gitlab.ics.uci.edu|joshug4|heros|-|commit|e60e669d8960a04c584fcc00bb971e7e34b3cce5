 public AccessPath<T> removeFirst() {










 T[] newAccesses = (T[]) new Object[accesses.length-1];










 System.arraycopy(accesses, 1, newAccesses, 0, accesses.length-1);










 return new AccessPath<>(newAccesses, exclusions);










 }










 










