 public boolean isAccessInExclusions(SubAccessPath<T>... fieldReferences) {










 if(fieldReferences.length > 0) {










 for(T field : fieldReferences[0].elements()) {










 if(!exclusions.contains(field))










 return false;










 }










 return true;










 }










 return false;










 }










 










